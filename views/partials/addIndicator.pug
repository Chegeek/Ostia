mixin addIndicator(indicator, id_)
  form.form-inline(id=id_)
    //- .checkbox
    //-   label
    //-     input(type='checkbox')
    .form-group
      label(for='inputEmail')= indicator
      input.form-control(type='text', placeholder='Param', required='', id=`addIndicator_${id_}`)
      //- .btn-toolbar
      button.btn.indcbtn(type='button', id=`addIndicatorBtn_${id_}`) Update
      //- button.btn.indcbtn(type='button', id='removeIndicator') X
